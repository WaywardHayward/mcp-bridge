{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "McpServers": {
    "Servers": {
      "example-echo": {
        "Transport": "Stdio",
        "Command": "npx",
        "Args": ["-y", "@anthropics/mcp-server-echo"],
        "WorkingDirectory": null,
        "Environment": null
      },
      "filesystem": {
        "Transport": "Stdio",
        "Command": "npx",
        "Args": ["-y", "@modelcontextprotocol/server-filesystem", "/tmp"]
      },
      "your-local-server": {
        "Transport": "Stdio",
        "Command": "node",
        "Args": ["/path/to/your/server/dist/index.js"],
        "Environment": {
          "MY_API_URL": "http://localhost:8080"
        }
      },
      "remote-sse-server": {
        "Transport": "Sse",
        "Url": "https://your-mcp-server.example.com/v1/sse",
        "ApiKeyEnvVar": "YOUR_API_KEY_ENV_VAR",
        "Headers": {
          "X-Custom-Header": "optional-header-value"
        }
      }
    }
  }
}
