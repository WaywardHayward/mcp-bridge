{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "McpServers": {
    "Servers": {
      "example-echo": {
        "Transport": "Stdio",
        "Command": "npx",
        "Args": ["-y", "@anthropics/mcp-server-echo"]
      },
      "filesystem": {
        "Transport": "Stdio",
        "Command": "npx",
        "Args": ["-y", "@modelcontextprotocol/server-filesystem", "/tmp"]
      }
    }
  }
}
